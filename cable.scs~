concept_cable => nrel_main_idtf :[кабель] (* <- lang_ru;; *);;
concept_cable => nrel_main_idtf :[cable](* <- lang_en;; *);;

//определение 

concept_cable <- rrel_key_sc_element: 
        ...
	(*
	=> nrel_main_idtf: [Опр.(кабель)]  (* <- lang_ru;; *);;
	<- sc_explanation;; 
	<= nrel_sc_text_translation: 
		...
		(*
		-> rrel_example: 
			[Кабель — конструкция из одного или нескольких изолированных друг от друга проводников (жил), или оптических волокон, 				заключённых в оболочку.]
			(* <- lang_ru;; *);;
		*);; 
	*);;

